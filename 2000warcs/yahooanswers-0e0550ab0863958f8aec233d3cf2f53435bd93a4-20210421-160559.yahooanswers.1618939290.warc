Anonymous":false,"commentCount":0,"createdTime":"2006-10-18T12:19:18Z","userAnswerRelation":{"hasFlagged":false,"canFlag":true,"canVote":false,"canChooseBestAnswer":false,"hasVoted":false,"isAuthor":false,"hasCommented":false,"canComment":true}},{"qid":"20061018121347AA8XL14","id":"2K6HGmhPx1pzrR-psS62DwFFyTGju4gOxSP4KRoSoWO8JrTzGZT_zrnuyxLoNGa9OJKG4BgDuswK10GlmIJor3I=","text":"Jimmy Stewart is the best actor ever,\r\nthough I don't really like some of his movies. It's not him though, he's great, it's the plot.","attachedImageUrl":null,"attachedImageId":null,"reference":null,"answerer":{"euid":"AbIbrvRqnURf6VfK6Osr1YgA73_dWp3WWnL1u0Lxzw--","kid":"abd7e42b0a8fb21ec360a23be59e9789aa","nickname":"Lady Ettejin of Wern","imageUrl":"https://s.yimg.com/dh/ap/social/profile/profile_b48.png","level":6},"isBestAnswer":false,"thumbsDown":0,"thumbsUp":0,"isAnonymous":false,"commentCount":0,"createdTime":"2006-10-18T12:46:22Z","userAnswerRelation":{"hasFlagged":false,"canFlag":true,"canVote":false,"canChooseBestAnswer":false,"hasVoted":false,"isAuthor":false,"hasCommented":false,"canComment":true}},{"qid":"20061018121347AA8XL14","id":"XXebLmVPx1qN-qhDKse8pFwrSuJ3JbVQi0mJr6KLPR2mdtNx9CplsNCE2oR5lX3hmNR-juuu6Cl1VoDg4_XMW-M=","text":"Amitab Bacchan,Bollywood","attachedImageUrl":null,"attachedImageId":null,"reference":null,"answerer":{"euid":"SkowQUJshSf_frm4raSRGM_bg.MK9k7cz30ATp8HNQ--","kid":"89c63ecb1c579112232659e5273c196aaa","nickname":"geniuswithU","imageUrl":"https://s.yimg.com/ag/images/default_user_profile_pic_128sq.jpg","level":2},"isBestAnswer":false,"thumbsDown":1,"thumbsUp":0,"isAnonymous":false,"commentCount":0,"createdTime":"2006-10-18T12:16:19Z","userAnswerRelation":{"hasFlagged":false,"canFlag":true,"canVote":false,"canChooseBestAnswer":false,"hasVoted":false,"isAuthor":false,"hasCommented":false,"canComment":true}}],"isServerFetched":false},"error":false}

