y that you moved him.&quot;,&quot;attachedImageUrl&quot;:null,&quot;attachedImageId&quot;:null,&quot;reference&quot;:null,&quot;answerer&quot;:{&quot;euid&quot;:&quot;4JQRorRvgVO28KS0VIFB5.RmZ_AIovXjfLB2j.1M7A--&quot;,&quot;kid&quot;:&quot;F6M6hau6aa&quot;,&quot;nickname&quot;:&quot;?&quot;,&quot;imageUrl&quot;:&quot;https://s.yimg.com/dh/ap/social/profile/profile_b48.png&quot;,&quot;level&quot;:1},&quot;isBestAnswer&quot;:false,&quot;thumbsDown&quot;:0,&quot;thumbsUp&quot;:0,&quot;isAnonymous&quot;:false,&quot;commentCount&quot;:0,&quot;createdTime&quot;:&quot;2012-06-21T07:58:30Z&quot;,&quot;userAnswerRelation&quot;:{&quot;hasFlagged&quot;:false,&quot;canFlag&quot;:true,&quot;canVote&quot;:false,&quot;canChooseBestAnswer&quot;:false,&quot;hasVoted&quot;:false,&quot;isAuthor&quot;:false,&quot;hasCommented&quot;:false,&quot;canComment&quot;:true}}],&quot;isFetching&quot;:false,&quot;isRefreshing&quot;:false,&quot;isDeletingAnswer&quot;:false,&quot;deleteAnswerError&quot;:null,&quot;isServerFetched&quot;:true}},&quot;answerCommentsList&quot;:{&quot;TWIMnFNPx1rU9v5ZHEaNcgJG40fhZfmHe5eT6tMhrocXA9F1eIr92jg=&quot;:{&quot;aid&quot;:&quot;TWIMnFNPx1rU9v5ZHEaNcgJG40fhZfmHe5eT6tMhrocXA9F1eIr92jg=&quot;,&quot;isPostingComment&quot;:false,&quot;isPostCommentDone&quot;:false,&quot;commentCount&quot;:0,&quot;comments&quot;:[],&quot;postCommentError&quot;:null,&quot;isFetching&quot;:false},&quot;E7dO3hBPx1oX5LG8CDWWzjL6H5Ly5ldjMVIBJg1bNrtx2S8BoXm27jI=&quot;:{&quot;aid&quot;:&quot;E7dO3hBPx1oX5LG8CDWWzjL6H5Ly5ldjMVIBJg1bNrtx2S8BoXm27jI=&quot;,&quot;isPostingComment&quot;:false,&quot;isPostCommentDone&quot;:false,&quot;commentCount&quot;:0,&quot;comments&quot;:[],&quot;postCommentError&quot;:null,&quot;isFetching&quot;:false},&quot;jKuxJnFPx1q9JjtIFwYOEUPRzBA5Q0sOp26nb3uEklfJwhOyG6Hcy6I=&quot;:{&quot;aid&quot;:&quot;jKuxJnFPx1q9JjtIFwYOEUPRzBA5Q0sOp26nb3uEklfJwhOyG6Hcy6I=&quot;,&quot;isPostingComment&quot;:false,&quot;isPostCommentDone&quot;:false,&quot;commentCount&quot;:0,&quot;c